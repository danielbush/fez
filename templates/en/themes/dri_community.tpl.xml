<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://di.tamu.edu/DRI/1.0/" xmlns:i18n="http://apache.org/cocoon/i18n/2.1"
    version="1.0">
    <body>
        <div rend="primary repository community" n="{$action}"
            id="artifactbrowser.CommunityViewer.div.community-home">
            <head>{$head}</head>
            <div rend="secondary search-browse" n="community-search-browse"
                id="artifactbrowser.CommunityViewer.div.community-search-browse">
                <div interactive="yes" rend="secondary search"
                    action="{$action}" n="community-search" method="post"
                    id="artifactbrowser.CommunityViewer.div.community-search">
                    <p n="search-query" id="artifactbrowser.CommunityViewer.p.search-query">Full
                        Text Search: <field type="text" n="query"
                            id="artifactbrowser.CommunityViewer.field.query">
                            <params/>
                        </field>
                        <field type="button" n="submit"
                            id="artifactbrowser.CommunityViewer.field.submit">
                            <params/>
                            <value type="raw">Go</value>
                        </field>
                    </p>
                </div>
                <div rend="secondary browse" n="community-browse"
                    id="artifactbrowser.CommunityViewer.div.community-browse">
                    <list rend="community-browse" type="simple" n="community-browse"
                        id="artifactbrowser.CommunityViewer.list.community-browse">
                        <head>Browse by</head>
                        <item>
                            <xref target="/manakin/handle/123456789/3/browse-title">Titles</xref>
                        </item>
                        <item>
                            <xref target="/manakin/handle/123456789/3/browse-author">Authors</xref>
                        </item>
                        <item>
                            <xref target="/manakin/handle/123456789/3/browse-date">Dates</xref>
                        </item>
                    </list>
                </div>
            </div>
            <div rend="secondary" n="community-view"
                id="artifactbrowser.CommunityViewer.div.community-view">
                <includeSet type="detailView" n="community-view"
                    id="artifactbrowser.CommunityViewer.includeSet.community-view">
                    <objectInclude repositorySource="hdl:123456789" objectSource="hdl:123456789/3">
                        <includeSet rend="hierarchy" type="summaryList">
                            <head>Collections in this community</head>
                            {section name=i loop=$list} 
                            <objectInclude repositorySource="hdl:123456789"
                            objectSource="{$list[i].rek_pid}"/>
                            {/section}
                        </includeSet>
                    </objectInclude>
                </includeSet>
            </div>
            
        </div>
    </body>
    <options>
        <list n="browse" id="artifactbrowser.Navigation.list.browse">
            <head>Browse</head>
            <list n="global" id="artifactbrowser.Navigation.list.global">
                <head>All of DSpace</head>
                <item>
                    <xref target="/manakin/community-list">Communities &amp; Collections</xref>
                </item>
                <item>
                    <xref target="/manakin/browse-title">Titles</xref>
                </item>
                <item>
                    <xref target="/manakin/browse-author">Authors</xref>
                </item>
                <item>
                    <xref target="/manakin/browse-subject">Subjects</xref>
                </item>
                <item>
                    <xref target="/manakin/browse-date">By Dates</xref>
                </item>
            </list>
            <list n="context" id="artifactbrowser.Navigation.list.context">
                <head>This Community</head>
                <item>
                    <xref target="/manakin/handle/123456789/3/browse-title">Titles</xref>
                </item>
                <item>
                    <xref target="/manakin/handle/123456789/3/browse-author">Authors</xref>
                </item>
                <item>
                    <xref target="/manakin/handle/123456789/3/browse-subject">Subjects</xref>
                </item>
                <item>
                    <xref target="/manakin/handle/123456789/3/browse-date">By Dates</xref>
                </item>
            </list>
        </list>
        <list n="account" id="artifactbrowser.Navigation.list.account">
            <head>My Account</head>
            <item>
                <xref target="/manakin/login">Login</xref>
            </item>
            <item>
                <xref target="/manakin/register">Register</xref>
            </item>
        </list>
        <list n="context" id="artifactbrowser.Navigation.list.context"/>
        <list n="administrative" id="artifactbrowser.Navigation.list.administrative"/>
    </options>
    <meta>
        <userMeta authenticated="{if $isAdministrator}yes{else}no{/if}">
            <metadata element="resource" qualifier="collection_admin">{if
                $isAdministrator}yes{else}no{/if}</metadata>
            <metadata element="identifier" qualifier="loginURL">login.php</metadata>
            <metadata element="language" qualifier="RFC3066">en_US</metadata>
            <metadata element="language" qualifier="RFC3066">en</metadata>
            <!-- user authenticated to edit theme -->
        </userMeta>
        <pageMeta>
            <metadata element="contextPath"/>
            <metadata element="theme" qualifier="path">themes/{$theme_id}</metadata>
            <metadata element="request" qualifier="queryString"/>
            <metadata element="request" qualifier="scheme">{$smarty.const.APP_SCHEME}</metadata>
            <metadata element="request" qualifier="serverPort">{$smarty.const.APP_PORT}</metadata>
            <metadata element="request" qualifier="serverName">{$smarty.const.APP_HOSTNAME}</metadata>
            <metadata element="server" qualifier="url">{$smarty.const.APP_BASE_URL}</metadata>
            <metadata element="request" qualifier="URI">dri.php?action=browse-title&amp;pid={$pid}</metadata>
            <metadata element="search" qualifier="simpleURL">dri.php?action=search</metadata>
            <metadata element="search" qualifier="advancedURL">dri.php?action=adv_search</metadata>
            <metadata element="search" qualifier="queryField">query</metadata>
            <metadata element="page" qualifier="contactURL">dri.php?action=contact</metadata>
            <metadata element="page" qualifier="feedbackURL">dri.php?action=feedback</metadata>
            <metadata element="focus" qualifier="container">{$parent_pid}</metadata>
            <metadata element="title">Browse By Title</metadata>
            <trail target="/manakin/">DSpace Home</trail>
            <trail target="/manakin/handle/123456789/1">African Journal of Information &amp;
                Communication Technology</trail>
            <trail target="/manakin/handle/123456789/301">Volume 1 Issue 1</trail>
            <trail>Browse By Title</trail>
        </pageMeta>
        
        <objectMeta>
            <object objectIdentifier="hdl:123456789/3" url="/manakin/handle/123456789/3"
                repositoryIdentifier="hdl:123456789">
                <mets:METS xmlns:mets="http://www.loc.gov/METS/"
                    xmlns:xlink="http://www.w3.org/TR/xlink/"
                    xmlns:dim="http://www.dspace.org/xmlns/dspace/dim" LABEL="DSpace Community"
                    PROFILE="DSPACE METS SIP Profile 1.0 (DRI extensions for DIM and communities/collections)"
                    ID="hdl:123456789/3">
                    <mets:dmdSec GROUPID="group_dmd_0" ID="dmd_1">
                        <mets:mdWrap OTHERMDTYPE="DIM" MDTYPE="OTHER">
                            <mets:xmlData>
                                <dim:dim dspaceType="COMMUNITY">
                                    <dim:field element="description" mdschema="dc"/>
                                    <dim:field element="description" mdschema="dc"
                                        qualifier="abstract"/>
                                    <dim:field element="description" mdschema="dc"
                                        qualifier="tableofcontents"/>
                                    <dim:field element="identifier" mdschema="dc" qualifier="uri"
