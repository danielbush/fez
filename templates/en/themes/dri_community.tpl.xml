                                </dim:dim>
                            </mets:xmlData>
                        </mets:mdWrap>
                    </mets:dmdSec>
                    <mets:dmdSec GROUPID="group_dmd_0" ID="dmd_2">
                        <mets:mdWrap OTHERMDTYPE="DIM" MDTYPE="OTHER">
                            <mets:xmlData>
                                <dim xmlns="http://www.dspace.org/xmlns/dspace/dim"
                                    dspaceType="COLLECTION">
                                    <field mdschema="dc" element="description">{$list[i].rek_description|escape}</field>
                                    <field mdschema="dc" element="description" qualifier="abstract"
