                                </dim>
                            </mets:xmlData>
                        </mets:mdWrap>
                    </mets:dmdSec>
                    
                    <mets:fileSec>
                            {assign var="loop_format"
                            value=$list[i].files} 
                            {section name="f" loop=$loop_format}
                        <mets:fileGrp USE="{$list[i].files[f].use}"> 
                            <mets:file SIZE="3595" GROUP_ID="group_file_437"
                                CHECKSUM="ca20a6ab94866ce74767643ef2f12f3f" MIMETYPE="text/xml" CHECKSUMTYPE="MD5"
                                ID="file_437">
                                <mets:FLocat LOCTYPE="URL"
                                    xlink:href="{$app_base_url}eserv.php?pid={$list[i].rek_pid|escape}&amp;dsID={$list[i].files[f].rek_file_attachment_name|escape}"
                                    xlink:title="{$list[i].files[f].rek_file_attachment_name|escape}" xlink:type="locator"/>
                            </mets:file>
                        </mets:fileGrp>
                        {/section} 
                    </mets:fileSec>
                    
                    <mets:structMap TYPE="LOGICAL" LABEL="DSpace">
                        <mets:div TYPE="DSpace Collection" DMDID="dmd_1 dmd_2 dmd_2"/>
                    </mets:structMap>
                </mets:METS>
            </object>
            {/section}

        </objectMeta>
