        <p>Or enter the first few letters:<field type="text" n="startsWith"
            id="artifactbrowser.BrowseTitles.field.startsWith">
            <params/>
            <help>Browse for items that begin with this title.</help>
          </field>
          <field type="button" n="submit" id="artifactbrowser.BrowseTitles.field.submit">
            <params/>
            <value type="raw">Go</value>
          </field>
        </p>
      </div>
      <div rend="primary" itemsTotal="11" firstItemIndex="1" pagination="simple" lastItemIndex="11"
        n="browse-by-title-results" id="artifactbrowser.BrowseTitles.div.browse-by-title-results">
        <includeSet type="summaryList" orderBy="title" n="browse-by-title"
          id="artifactbrowser.BrowseTitles.includeSet.browse-by-title">
          {section name=i loop=$list} 
        <objectInclude repositorySource="{$list[i].rek_pid}" objectSource="{$list[i].rek_pid}"/>
        {/section}
