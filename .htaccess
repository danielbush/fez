RewriteEngine   on
RewriteRule     ^view/([A-Za-z]+):([0-9]+)(\?.*)*$          view.php?pid=$1:$2$3
RewriteRule     ^eserv/([A-Za-z]+):([0-9]+)/(.+)(/.+)*$     eserv.php?pid=$1:$2&dsID=$3$4
RewriteRule     ^community/([A-Za-z]+):([0-9]+)(\?.*)*$	    list.php?community_pid=$1:$2$3
RewriteRule     ^collection/([A-Za-z]+):([0-9]+)(\?.*)*$    list.php?collection_pid=$1:$2$3