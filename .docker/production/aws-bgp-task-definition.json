{
  "family": "fezbgp",
  "containerDefinitions": [
    {
      "name": "fpm",
      "image": "uql-registry.library.uq.edu.au/production/fez:<COMMIT_HASH>",
      "cpu": 256,
      "memory": 1024,
      "environment": [
        {
          "name": "APP_ENVIRONMENT",
          "value": "staging"
        },
        {
          "name": "NEWRELIC_LICENSE",
          "value": "<NEWRELIC_LICENSE>"
        },
        {
          "name": "BGP_ID",
          "value": "<BGP_ID>"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "tmp",
          "containerPath": "/var/app/tmp"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [{
    "name": "tmp",
    "host": {}
  }]
}